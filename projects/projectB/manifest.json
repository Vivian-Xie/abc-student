{
  "manifest_version": 3,
  "name": "link-eater",
  "version": "1.0",
  "description": "a lizard that eats link",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css":[
        "style.css"
      ],
      "matches": [
        "<all_urls>"
      ]
      
    }
  ],
  "icons": {
    "16": "icon.png"

  },
  "permissions": [
    "tabs",
    "storage",
    "system.display"
  ],
  "background": {
    "serivce_worker": "background.js"
  }
}